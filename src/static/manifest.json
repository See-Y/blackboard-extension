{
  "name": "Blackboard Extension",
  "description": "Blackboard Extension from UNIST HeXA",
  "version": "3.0.0",
  "manifest_version": 3,
  "icons" : {
    "16": "HeXA_icon.png",
    "64": "HeXA_icon.png",
    "128": "HeXA_icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
    }
]
}
